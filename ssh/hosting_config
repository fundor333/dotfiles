	User git
	HostName github.com
	IdentityFile ~/.ssh/keys/fundor333_rsa
	AddKeysToAgent yes

Host bitbucket.org
	User git
	HostName bitbucket.org
	IdentityFile ~/.ssh/keys/fundor333_rsa
	AddKeysToAgent yes
